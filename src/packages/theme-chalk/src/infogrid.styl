.menu-time {
  font-size: 14px;
}
.tags {
  width: 100%;
  height: 100%;
}
.header {
  width: 100%;
  min-width: 200px;
  height: 44px;
  background-image: url(./assets/tag/ui-header-bg.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  display: flex;
  justify-content: space-between;
  line-height: 32px;
}
.important-souce-span {
  padding-left: 20px;
}
.top-row .no-img-title {
  width: calc(80% - 58px);
}
.header .title {
  font-size: 16px;
  color: rgb(254, 191, 21);
  padding-left: 7%;
}
.header .more {
  color: white;
  margin-right: 6%;
  cursor: pointer;
}
.section {
  width: 100%;
  min-width: 200px;
  height: calc(100% - 47px);
  overflow: auto;
}
.important-msg {
  height: 112px;
  display: flex;
  margin: 0 3%;
  cursor: pointer;
  border-bottom: 1px solid rgba(17, 138, 191, 0.3);
  margin-top: 10px;
}
.important-img {
  width: 142px;
  height: 100px;
  box-sizing: border-box;
  border: 2px rgb(29, 88, 183) solid;
  border-radius: 4px;
}
.important-img img {
  width: 100%;
  height: 100%;
  background-size: 100% 100%;
}
.important-title {
  font-size: 18px;
  max-width: 75%;
  color: white;
  line-height: 25px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.important-souce {
  font-size: 14px;
  color: rgb(208, 208, 208);
  line-height: 18px;
  display: flex;
  justify-content: space-between;
}
.normsg {
  height: 34px;
  line-height: 34px;
  border-bottom: 1px solid rgba(17, 138, 191, 0.3);
  margin: 0 3%;
  cursor: pointer;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-right: 10px;
  display: flex;
  align-items: center;
  font-size: 16px;
}
.no-img-title {
  display: block;
  width: 80%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.normsg:hover .no-img-title,
.important-msg:hover .important-title {
  color: rgb(255, 215, 93);
}
.important-info {
  margin-left: 3%;
  margin-right: 5%;
  flex: 1;
}
.important-info ol {
  display: flex;
  color: white;
  justify-content: space-between;
  margin-top: 8px;
}
.important-info ol li {
  display: flex;
  align-items: center;
  font-size: 12px;
}
.important-info ol span {
  display: inline-block;
  width: 15px;
  height: 14px;
  margin-right: 4px;
}
.icon {
  background-image: url(./assets/tag/icon.png);
}
.star {
  background-position: left 1px;
}
.eye {
  background-position: 35px 1px;
}
.good {
  background-position: 56px 1px;
}
.msg {
  background-position: right 0px;
}
.widget-wrapper {
  width: 100%;
  height: 100%;
  border: 1px solid rgba(17, 138, 191, 0.9);
  box-sizing: border-box;
  background: -moz-linear-gradient(-80deg, #023075, #04134c);
  color: white;
}

.widget-header-wrapper {
  height: 36px;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 12px;
}

.header-text {
  font-size: 18px;
  color: #ffde00;
  margin-left: 20px;
}

.widget-header-wrapper::after {
  content: '';
  position: absolute;
  bottom: -12px;
  height: 12px;
  left: 0;
  right: 0;
  border: 1px solid #1c44a9;
  background: url(./assets/direction.png) repeat-x;
  background-size: 100%;
  background-position: center;
}

.readmore {
  font-size: 18px;
  font-weight: bold;
  background-image: url(./assets/bg_btn_more@59x26.png);
  background-repeat: no-repeat;
  background-position: right center;
  padding: 0 15px;
  margin-left: auto;
  margin-right: 16px;
  cursor: pointer;
}

.widget-wrapper > * {
  margin-left: auto;
  margin-right: auto;
}
.top-able {
  content: '置顶';
  border-radius: 2px;
  width: 52px;
  height: 24px;
  background: #ea505d;
  margin-right: 6px;
  border: 1px solid rgba(255, 255, 255, 0.6);
  box-sizing: border-box;
  transform: rotate(0deg);
  vertical-align: baseline;
  text-align: center;
  line-height: 20px;
  color: rgb(245, 245, 245);
  display: inline-block;
}
.readmore .more {
  display: none;
}

/**皮肤1*/
.widget-wrapper.border1 {
  border-style: solid;
  border-color: transparent;
  background-image: url(./assets/border/border-type1.png), url(./assets/border/border-type2.png),
    url(./assets/border/border-type3.png), url(./assets/border/border-type4.png),
    url(./assets/border/border-type5.png), url(./assets/border/border-type6.png),
    url(./assets/border/border-type7.png), url(./assets/border/border-type8.png),
    url(./assets/border/border-type9.png);
  background-repeat: no-repeat;
  background-position: left top, 180px top, right top, left 65px, 180px 65px, right 65px, left bottom, 180px bottom,
    right bottom;
  background-size: auto, calc(100% - 180px - 182px) 65px, auto, 180px calc(100% - 65px - 40px),
    calc(100% - 180px - 182px) calc(100% - 65px - 40px), 182px calc(100% - 65px - 40px), auto,
    calc(100% - 180px - 182px) 40px, auto;
  box-sizing: border-box;
  padding: 0 5px 0 0;
}
.border1 .readmore .more {
  display: inline;
}
.border1 .widget-header-wrapper {
  margin: 12px 28px 0 30px;
  border-bottom: 1px rgba(0, 198, 221, 0.14) solid;
  height: 42px;
  box-sizing: border-box;
}
.border1 .widget-header-wrapper::after {
  content: '';
  position: none;
  bottom: -12px;
  height: 12px;
  left: 0;
  right: 0;
  border: none;
  background: none;
  background-size: none;
  background-position: none;
}
.border1 .header-text {
  font-size: 18px;
  color: rgb(239, 197, 25);
  height: 37px;
  line-height: 37px;
  margin-left: 0px;
  position: relative;
  border-bottom: 2px rgb(76, 173, 255) solid;
  max-width: 65%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.border1 .readmore {
  font-size: 16px;
  font-weight: normal;
  background-image: none;
  background-repeat: no-repeat;
  background-position: right center;
  padding: 0px;
  height: 37px;
  line-height: 37px;
  margin-left: auto;
  margin-right: 0px;
  cursor: pointer;
  color: rgb(85, 147, 255);
  position: relative;
}
.border1 .readmore::after {
  content: '';
  position: absolute;
  display: block;
  width: 7px;
  height: 2px;
  background-color: rgb(76, 173, 255);
  top: 36px;
  right: 0px;
}
.border1 .important-img {
  width: 100px;
  height: 70px;
  border: 1px rgb(29, 88, 183) solid;
}
.border1 .important-title {
  line-height: 40px;
}
.border1 .important-msg {
  height: 82px;
  margin-top: 17px;
  border-bottom: 1px dashed rgba(0, 144, 255, 0.3);
}
.border1 .important-souce {
  display: none;
}
.border1 .important-info ol {
  opacity: 0.6;
}
.border1 .normsg {
  border-bottom: 1px dashed rgba(0, 144, 255, 0.3);
  height: 55px;
  box-sizing: border-box;
}
.border1 .no-img-title {
  width: 89%;
}
.border1 .menu-time {
  color: rgb(184, 188, 201);
}
.border1 .section ul li {
  padding-left: 5px;
}
.border1 .section.gridtype {
  padding: 0;
  height: calc(100% - 60px);
}

