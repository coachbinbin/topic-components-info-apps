  border-bottom: 1px rgba(0, 198, 221, 0.14) solid;
  height: 42px;
  box-sizing: border-box;
}
.border1 .readmore {
  font-size: 16px;
  font-family: 'TIM';
  font-weight: normal;
  background-image: none;
  background-repeat: no-repeat;
  background-position: right center;
  padding: 0px;
  height: 37px;
  line-height: 37px;
  margin-left: auto;
  margin-right: 0px;
  cursor: pointer;
  color: rgb(85, 147, 255);
  position: relative;
}
.border1 .readmore .more {
  display: inline;
}
.border1 .readmore::after {
  content: '';
  position: absolute;
  display: block;
  width: 7px;
  height: 2px;
  background-color: rgb(76, 173, 255);
  top: 36px;
  right: 0px;
}
.border1 .section {
  padding: 6px 26px 12px;
  height: calc(100% - 60px);
  font-family: 'TIR';
}
.border1 .widget-header-wrapper::after {
  content: '';
  position: none;
  bottom: -12px;
  height: 12px;
  left: 0;
  right: 0;
  border: none;
  background: none;
  background-size: none;
  background-position: none;
}

