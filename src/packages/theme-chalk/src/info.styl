.widget-wrapper {
  width: 100%;
  height: 100%;
  border: 1px solid rgba(17, 138, 191, 0.9);
  box-sizing: border-box;
  background: -moz-linear-gradient(-80deg, #023075, #04134c);
  overflow: hidden;
}
.section {
  width: 100%;
  min-width: 199px;
  height: calc(100% - 47px);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  overflow: auto;
  padding: 6px 16px 12px;
  box-sizing: border-box;
}

.section {
  width: 100%;
  min-width: 199px;
  height: calc(100% - 47px);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  overflow: auto;
  padding: 6px 16px 12px;
  box-sizing: border-box;
}
.menu-list {
  border-bottom: 1px solid rgba(17, 138, 191, 0.3);
  padding-left: 10px;
  width: 100%;
  height: 40px;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #ffffff;
  cursor: pointer;
}
.menu-list:hover .menu-title {
  color: rgb(255, 215, 93);
}
.menu-title {
  font-size: 16px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  cursor: pointer;
  display: flex;
  align-items: center;
}
.menu-title span {
  display: inline-block;
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background-color: yellow;
  margin: 2px 8px;
  flex-shrink: 0;
}
.menu-time {
  margin-left: 18px;
  margin-right: 6px;
  font-size: 14px;
  color: white;
}
.menu-title .top-able {
  content: '';
  border-radius: 2px;
  width: 52px;
  height: 24px;
  background: #ea505d;
  margin-right: 6px;
  border: 1px solid rgba(255, 255, 255, 0.6);
  box-sizing: border-box;
  transform: rotate(0deg);
  vertical-align: baseline;
  text-align: center;
  line-height: 20px;
  color: rgb(245, 245, 245);
  display: inline-block;
}
.title-mains {
  margin: 0;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.widget-header-wrapper {
  height: 36px;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 12px;
}

.header-text {
  font-size: 18px;
  color: #ffde00;
  margin-left: 20px;
}

.widget-header-wrapper::after {
  content: '';
  position: absolute;
  bottom: -12px;
  height: 12px;
  left: 0;
  right: 0;
  border: 1px solid #1c44a9;
  background: url(./assets/direction.png) repeat-x;
  background-size: 100%;
  background-position: center;
}

.readmore {
  font-size: 18px;
  font-weight: bold;
  background-image: url(./assets/bg_btn_more@59x26.png);
  background-repeat: no-repeat;
  background-position: right center;
  padding: 0 15px;
  margin-left: auto;
  margin-right: 16px;
  cursor: pointer;
}
.readmore .more {
  display: none;
}
