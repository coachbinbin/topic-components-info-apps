.widget-wrapper {
  width: 100%;
  height: 100%;
  border: 1px solid rgba(17, 138, 191, 0.9);
  box-sizing: border-box;
  background: -moz-linear-gradient(-80deg, #023075, #04134c);
  overflow: hidden;
}

.title {
  height: 48px;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  padding: 0 22px;
  font-family: 'Alibaba PuHuiTi Medium';
  margin-bottom: 5px;
}

.title-bottom {
  height: 2px;
  width: calc(100% - 44px);
  background-color: #033a78;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
}

.title-bottom > * {
  background-color: #4cadff;
  height: 1px;
  border-bottom: 1px solid #3385d0;
}

.title-text {
  color: #ffd015;
  font-size: 18px;
}

.title-more {
  color: #5593ff;
  font-size: 16px;
  cursor: pointer;
}

.main-section-tags {
  height: calc(100% - 48px);
  /* width: 500px; */
  width: 100%;
  margin: 0 auto;
  position: relative;
  /* margin-top: 20px; */
}

.main-scroll {
  overflow-y: auto;
  height: 100%;
  padding: 0 15px;
}
.main-scroll .main-section-item:last-child {
  margin-bottom: 15px;
}
.main-section-item {
  height: 130px;
  border-bottom: 1px solid #0a5790;
  color: #fff;
  cursor: pointer;
}

.item-title {
  height: 40px;
  line-height: 40px;
  width: 82%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-indent: 10px;
  font-size: 16px;
}

.item-center {
  height: calc(100% - 50px);
  margin-top: 5px;
  padding: 0 38px 0 10px;
  display: flex;
  justify-content: space-between;
}

.item-center img {
  width: 100px;
  height: 100%;
  border: 1px solid #3d7cde;
  border-radius: 5px;
  box-sizing: border-box;
}

.item-detail {
  width: calc(100% - 112px);
}

.item-detail-content {
  height: 44px;
  line-height: 22px;
  font-size: 14px;
  margin: 5px 0;
  overflow: hidden;
  word-break: break-all;
}

.item-detail-bottom {
  height: calc(100% - 54px);
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.item-detail-bottom-right {
  display: flex;
  min-width: 265px;
  width: calc(100% - 80px);
  justify-content: flex-end;
  align-items: center;
}

.item-detail-bottom-right span {
  display: inline-block;
  width: 15px;
  height: 14px;
  margin-right: 4px;
}

.icon {
  background-image: url(./assets/tag/icon.png);
}
.star {
  background-position: left 1px;
}
.eye {
  background-position: 35px 1px;
}
.good {
  background-position: 56px 1px;
}
.msg {
  background-position: right 0px;
}

.item-detail-bottom-right > * {
  margin-left: 15px;
}
.item-detail-bottom img {
  width: auto;
  height: auto;
  border: none;
  border-radius: 0;
  position: relative;
  top: 2px;
  cursor: pointer;
}

.widget-header-wrapper {
  height: 36px;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 12px;
}

.header-text {
  font-size: 18px;
  color: #ffde00;
  margin-left: 20px;
}

.widget-header-wrapper::after {
  content: '';
  position: absolute;
  bottom: -12px;
  height: 12px;
  left: 0;
  right: 0;
  border: 1px solid #1c44a9;
  background: url(./assets/direction.png) repeat-x;
  background-size: 100%;
  background-position: center;
}

.readmore {
  font-size: 18px;
  font-weight: bold;
  background-image: url(./assets/bg_btn_more@59x26.png);
  background-repeat: no-repeat;
  background-position: right center;
  padding: 0 15px;
  margin-left: auto;
  margin-right: 16px;
  cursor: pointer;
}
.readmore .more {
  display: none;
}
.main-section-item:hover .item-title {
  color: rgb(255, 215, 93);
}
/**皮肤1*/
.widget-wrapper.border1 {
  border-style: solid;
  border-color: transparent;
  background-image: url(./assets/border/border-type1.png), url(./assets/border/border-type2.png),
    url(./assets/border/border-type3.png), url(./assets/border/border-type4.png),
    url(./assets/border/border-type5.png), url(./assets/border/border-type6.png),
    url(./assets/border/border-type7.png), url(./assets/border/border-type8.png),
    url(./assets/border/border-type9.png);
  background-repeat: no-repeat;
  background-position: left top, 180px top, right top, left 65px, 180px 65px, right 65px, left bottom, 180px bottom,
    right bottom;
  background-size: auto, calc(100% - 180px - 182px) 65px, auto, 180px calc(100% - 65px - 40px),
    calc(100% - 180px - 182px) calc(100% - 65px - 40px), 182px calc(100% - 65px - 40px), auto,
    calc(100% - 180px - 182px) 40px, auto;
  box-sizing: border-box;
  padding: 0 5px 0 0;
}
.border1 .readmore .more {
  display: inline;
}
.border1 .widget-header-wrapper {
  margin: 12px 28px 0 30px;
  border-bottom: 1px rgba(0, 198, 221, 0.14) solid;
  height: 42px;
  box-sizing: border-box;
}
.border1 .widget-header-wrapper::after {
  content: '';
  position: none;
  bottom: -12px;
  height: 12px;
  left: 0;
  right: 0;
  border: none;
  background: none;
  background-size: none;
  background-position: none;
}
.border1 .header-text {
  font-size: 18px;
  color: rgb(239, 197, 25);
  height: 37px;
  line-height: 37px;
  margin-left: 0px;
  position: relative;
  border-bottom: 2px rgb(76, 173, 255) solid;
  max-width: 65%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.border1 .readmore {
  font-size: 16px;
  font-weight: normal;
  background-image: none;
  background-repeat: no-repeat;
  background-position: right center;
  padding: 0px;
  height: 37px;
  line-height: 37px;
  margin-left: auto;
  margin-right: 0px;
  cursor: pointer;
  color: rgb(85, 147, 255);
  position: relative;
}
.border1 .readmore::after {
  content: '';
  position: absolute;
  display: block;
  width: 7px;
  height: 2px;
  background-color: rgb(76, 173, 255);
  top: 36px;
  right: 0px;
}
.border1 .main-section-tags {
  height: calc(100% - 65px);
}
