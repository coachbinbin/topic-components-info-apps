  background-size: auto, calc(100% - 180px - 123px) 65px, auto, 180px calc(100% - 65px - 27px),
    calc(100% - 180px - 123px) calc(100% - 65px - 27px), 123px calc(100% - 65px - 27px), auto,
    calc(100% - 180px - 123px) 27px, auto;
  box-sizing: border-box;
  padding: 0 5px 0 0;
}
.border2 .header-text {
  font-size: 18px;
  color: rgb(239, 197, 25);
  height: 45px;
  line-height: 55px;
  margin-left: 0px;
  position: relative;
  max-width: 65%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.border2 .widget-header-wrapper {
  margin-left: 45px;
  border-bottom: none;
  height: 45px;
  line-height: 55px;
  box-sizing: border-box;
  margin-bottom: 0;
}
.border2 .readmore {
  font-size: 18px;
  line-height: 55px;
  font-weight: bold;
  background-image: url(./assets/bg_btn_more@59x26.png);
  background-repeat: no-repeat;
  background-position: right 16px;
  padding: 0px 15px;
  margin-left: auto;
  cursor: pointer;
  margin-top: 7px;
}
.border2 .readmore .more {
  display: none;
}
.border2 .readmore::after {
  content: '';
  position: absolute;
  display: none;
  width: 0px;
  height: 0px;
}
.border2 .section {
  padding: 6px 26px 12px;
  height: calc(100% - 50px);
}
.border2 .widget-header-wrapper::after {
  content: '';
  position: none;
  bottom: -12px;
  height: 12px;
  left: 0;
  right: 0;
  border: none;
  background: none;
  background-size: none;
  background-position: none;
}
